{
  "name": "cloud-lab",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.js",
  "repository": "git@github.com:carbon-design-system/carbon-tutorial.git",
  "bugs": "https://github.com/carbon-design-system/carbon-tutorial/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "react-scripts build",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@carbon/grid": "^10.9.2",
    "@carbon/icons": "^10.9.2",
    "@carbon/icons-react": "^10.9.2",
    "apollo-boost": "^0.4.2",
    "axios": "^0.19.1",
    "carbon-components": "^10.10.2",
    "carbon-components-react": "^7.10.2",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "express-pino-logger": "^4.0.0",
    "graphql": "^14.3.1",
    "is-empty": "^1.2.0",
    "jwt-decode": "^2.2.0",
    "pino": "^5.16.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-notifications": "^1.4.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.8",
    "react-simple-dropdown": "^3.2.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.14.0",
    "prettier": "^1.17.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "eqeqeq": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
